<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_shared_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    android:orientation="vertical">


    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <!--android:layout_alignParentBottom="true"-->


        <View
            android:id="@+id/line"
            style="@style/app_item_line_style"
            android:layout_width="match_parent"></View>


        <LinearLayout
            android:id="@+id/shared_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/line"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/wechat_friend"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/app_item_style"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="@dimen/item_padding">

                <ImageView
                    android:layout_width="@dimen/icon_size"
                    android:layout_height="@dimen/icon_size"
                    android:layout_marginTop="10dp"
                    android:src="@mipmap/wechat_friend" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/text_top"
                    android:text="@string/wechat_friend" />
            </LinearLayout>

            <View
                style="@style/app_item_line_style"
                android:layout_width="0.5dp"
                android:layout_height="match_parent">
            </View>

            <LinearLayout
                android:id="@+id/wechat_circle_friends"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/app_item_style"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="@dimen/item_padding">

                <ImageView
                    android:layout_width="@dimen/icon_size"
                    android:layout_height="@dimen/icon_size"
                    android:layout_marginTop="10dp"
                    android:src="@mipmap/wechat_circle_friends" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/text_top"
                    android:text="@string/wechat_circle_friends" />
            </LinearLayout>

            <View
                style="@style/app_item_line_style"
                android:layout_width="0.5dp"
                android:layout_height="match_parent">

            </View>

            <LinearLayout
                android:id="@+id/qq_friend"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/app_item_style"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="@dimen/item_padding">

                <ImageView
                    android:layout_width="@dimen/icon_size"
                    android:layout_height="@dimen/icon_size"
                    android:layout_marginTop="10dp"
                    android:src="@mipmap/qq_friend" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/text_top"
                    android:text="@string/qq_friend" />
            </LinearLayout>
            <View
                style="@style/app_item_line_style"
                android:layout_width="0.5dp"
                android:layout_height="match_parent">
            </View>
            <LinearLayout
            android:id="@+id/qq_Qzone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/app_item_style"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/item_padding">

            <ImageView
            android:layout_width="@dimen/icon_size"
            android:layout_height="@dimen/icon_size"
            android:layout_marginTop="10dp"
            android:src="@mipmap/qq_qzone" />

            <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/text_top"
            android:text="@string/qq_Qzone" />
            </LinearLayout>

        </LinearLayout>




    </RelativeLayout>
<!--<View-->
    <!--android:layout_alignParentBottom="true"-->
    <!--android:id="@+id/bottom_view"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="60dp"-->
    <!--android:background="#60BBCCDD"></View>-->
</RelativeLayout>


