<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="center"
    android:orientation="vertical">

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        android:src="@mipmap/icon_create_group_susseed" />

    <TextView
        style="@style/black_text_style"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="15dp"
        android:gravity="center"
        android:text="提交成功！" />

    <TextView
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="15dp"
        android:gravity="center"
        android:text="我们会尽快审核，请耐心等待。"
        android:textColor="@color/app_item_content"
        android:textSize="14sp"
        android:visibility="visible" />

    <View
        style="@style/app_item_line_style"
        android:layout_width="240dp" />

    <LinearLayout
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/app_left_right_size"
        android:layout_marginRight="@dimen/app_left_right_size">

        <!--<TextView-->
        <!--android:id="@+id/cancel"-->
        <!--android:layout_width="0dp"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_gravity="center"-->
        <!--android:layout_weight="1"-->
        <!--android:gravity="center"-->
        <!--android:text="@string/delete_order_cancel"-->
        <!--android:textColor="#63ABFF"-->
        <!--android:textSize="@dimen/app_item_title" />-->

        <!--<View-->
        <!--android:layout_width="1dp"-->
        <!--android:layout_height="match_parent"-->
        <!--android:background="@color/app_line" />-->

        <TextView
            android:id="@+id/sure"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:gravity="center"
            android:paddingBottom="15dp"
            android:paddingTop="15dp"
            android:text="@string/delete_order_sure"
            android:textColor="@color/app_special_style_color_normal"
            android:textSize="@dimen/app_item_title" />

    </LinearLayout>

</LinearLayout>