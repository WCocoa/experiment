<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    android:gravity="center_horizontal"
    >

    <!--/home/quliang/桌面/v1.2.1/img_apply_caption.png-->
    <!--/home/quliang/桌面/v1.2.1/img_apply_teacher.png-->
<!--</LinearLayout>-->
    <LinearLayout
        android:id="@+id/ll"
        android:layout_below="@+id/tag2"
        android:layout_marginTop="-40dp"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/img_apply_bg"
        android:padding="30dp"
        android:orientation="vertical"
        android:layout_width="275dp"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="15sp"
            android:layout_marginTop="55dp"
            android:id="@+id/name_tag"
            android:textColor="@color/app_item_title"
            android:text="@string/teacherjoin_d_name_tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <EditText
            android:textSize="15dp"
            android:maxLength="10"
            android:id="@+id/name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:textSize="15sp"
            android:textColor="@color/app_item_title"
            android:layout_marginTop="5dp"
            android:id="@+id/tel_tag"
            android:text="@string/teacherjoin_d_tel_tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <EditText
            android:textSize="15dp"
            android:inputType="number"
            android:numeric="integer"
            android:maxLength="11"
            android:digits="0123456879"
            android:id="@+id/tel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:textSize="14sp"
            android:layout_marginTop="15dp"
            android:id="@+id/label"
            android:text="@string/teacherjoin_d_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_marginTop="15dp"
            android:text="@string/teacherjoin_d_sure"
            android:id="@+id/sure"
            android:background="@drawable/teacher_join_btn_backgroud"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingBottom="10dp"
        android:paddingLeft="25dp"
        android:paddingRight="25dp"
        android:paddingTop="10dp"
        android:textColor="@color/white"
        android:textSize="@dimen/app_item_title"
        />
    </LinearLayout>



    <ImageView
        android:layout_centerHorizontal="true"
        android:id="@+id/tag2"
        android:layout_width="220dp"
        android:layout_height="100dp"
        android:background="@mipmap/img_apply_teacher" />
    <ImageView
        android:layout_marginTop="95dp"
        android:id="@+id/tag1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/img_apply_caption" />

    <ImageView
        android:layout_alignTop="@id/ll"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="-10dp"
        android:id="@+id/delete"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@mipmap/icon_apply_closebtn"
        />

</RelativeLayout>