<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowSoftInputMode">stateHidden</item>
    </style>

    <!-- 进度条 -->
    <style name="MyDialog" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <!--title 图标样式-->
    <style name="app_title_icon_style">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingLeft">@dimen/dimen_backicon_margin_left</item>
    </style>

    <!-- 标题样式 -->
    <style name="app_title_style">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/title</item>
        <item name="android:textColor">@color/app_title_text_bg</item>
    </style>
    <!--分割线样式-->
    <style name="app_item_line_style">
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/app_line</item>
    </style>

    <style name="Animation.Popup" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>

    <!-- 相册选择对话框样式 -->
    <style name="transparentFrameWindowStyle" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/dialog_photo_choose_bg</item>
    </style>

    <style name="main_menu_animstyle">
        <item name="android:windowEnterAnimation">@anim/dialog_photo_in_anim</item>
        <item name="android:windowExitAnimation">@anim/dialog_photo_out_anim</item>
    </style>

    <style name="app_button_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="app_pay_button_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/app_item_title</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="app_button_middle_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/login_btn_backgroud</item>
    </style>


    <style name="contentfontStyle">
        <item name="android:textColor">@color/app_item_content</item>
        <item name="android:textSize">12.0sp</item>
    </style>


    <!--title 图标样式-->
    <style name="home_category_img_style">
        <item name="android:layout_width">@dimen/home_category</item>
        <item name="android:layout_height">@dimen/home_category</item>
    </style>

    <style name="home_category_top_img_style">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
    </style>

    <!-- item 分割线 样式 -->
    <style name="city_item_line_style">
        <item name="android:layout_height">@dimen/app_line_hight</item>
        <item name="android:background">@color/city_line</item>
    </style>
    <!--右边箭头样式-->
    android:id="@+id/icon_right1"
    android:layout_width="15dp"
    android:layout_height="15dp"
    android:layout_alignParentRight="true"
    android:layout_centerVertical="true"
    android:layout_marginRight="@dimen/app_left_right_size"

    <style name="app_item_title_style">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/app_item_title</item>
    </style>

    <style name="app_item_content_style">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/app_item_content</item>
    </style>

    <style name="right_icon_style">
        <item name="android:layout_width">15dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/app_left_right_size</item>
        <item name="android:src">@mipmap/arrow_expend_right</item>
    </style>


    <!--commissionerrecruit_service_type-->
    <style name="commissionerrecruit_service_type_apply">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/app_special_style_color_normal</item>
        <item name="android:textSize">16sp</item>
        <item name="android:padding">15dp</item>
    </style>


    <style name="gray_text_style">
        <item name="android:textColor">@color/app_item_content</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="black_text_style">
        <item name="android:textColor">@color/app_item_title</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="red_star_style">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:text">*</item>
        <item name="android:textColor">@color/app_special_style_color_normal</item>
        <item name="android:textSize">18sp</item>
    </style>

    <!--CommissionerApplyOneActivity-->
    <style name="commissionerApplyActivity_label">
        <item name="android:textColor">@color/app_item_title</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="commissionerApplyActivity_content">
        <item name="android:textColor">@color/app_item_content</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="order_pay_text_style">
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textColor">@color/app_item_content</item>
        <item name="android:textSize">@dimen/app_item_title</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="order_pay_data_style">
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textColor">@color/app_item_title</item>
        <item name="android:textSize">@dimen/app_item_title</item>
    </style>

    <style name="MyDialogStyle1">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

</resources>
